      <tr>
        <td><para style="P19">En la ciudad de</para></td>
        <td><para style="P20">[[ _datos_parroquia['d'] or "-"]]</para></td>
		<td><para style="P19"> , República de Cuba,</para></td>
      </tr>
    </blockTable>
	<blockTable colWidths="22.0,190.0,323.0" style="Table3">
